                    <key>flapped[routers]</key>
                    <units>!time(s) has flapped</units>
                    <params>count(&quot;changes[router]&quot;,{$FLAP})</params>
                    <applications>
                        <application>
                            <name>Status of flapping</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Status of flapping[switches]</name>
                    <type>CALCULATED</type>
                    <key>flapped[switches]</key>
                    <units>!time(s) has flapped</units>
                    <params>count(&quot;changes[switch]&quot;,{$FLAP})</params>
                    <applications>
                        <application>
                            <name>Status of flapping</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Device down aggregated status</name>
                    <type>AGGREGATE</type>
                    <key>grpsum[&quot;{$CUSTOMER}&quot;,&quot;device.status.in.pool&quot;,&quot;last&quot;,&quot;#1&quot;]</key>
                    <delay>{$RD.STTS}</delay>
                    <applications>
                        <application>
                            <name>Device down dummy</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Device down dummy raw input</name>
                    <type>INTERNAL</type>
                    <key>zabbix[host,agent,&quot;available&quot;]</key>
                    <delay>{$RD.STTS}</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <applications>
                        <application>
                            <name>Device down dummy</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>zabbix.host.available</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>10m</params>
                        </step>
                    </preprocessing>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$CNT.ZA}</macro>
                    <value>0</value>
                    <description>Agents down more than this threshold</description>
                </macro>
                <macro>
                    <macro>{$FLAP}</macro>
                    <value>1d</value>
                    <description>The time period to analyze for flapping</description>
                </macro>
                <macro>
                    <macro>{$SNS}</macro>
                    <value>#1</value>
                    <description>Sensitivity. How many last values '#5' to analyze or the time period like '2h'</description>
                </macro>
            </macros>
        </template>
    </templates>
    <triggers>
        <trigger>
            <expression>{calc:down[switch].min({$SNS})}=1&#13;
and&#13;
{calc:down[router].min({$SNS})}=1</expression>
            <name>1 switch and 1 router down for {$CUSTOMER}</name>
            <opdata>{ITEM.LASTVALUE1}, {ITEM.LASTVALUE1} for {$CUSTOMER}</opdata>
            <priority>AVERAGE</priority>
            <manual_close>YES</manual_close>
            <dependencies>
                <dependency>
                    <name>1 or more firewalls down for {$CUSTOMER}</name>
                    <expression>{calc:down[firewall].min({$SNS})}&gt;0</expression>
                </dependency>
                <dependency>
                    <name>More than 1 router and more than 1 switch down for {$CUSTOMER}</name>
