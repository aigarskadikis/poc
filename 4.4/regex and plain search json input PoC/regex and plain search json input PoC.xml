<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2020-12-03T10:22:42Z</date>
    <groups>
        <group>
            <name>Templates/Modules</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>regex and plain search json input PoC</template>
            <name>regex and plain search json input PoC</name>
            <groups>
                <group>
                    <name>Templates/Modules</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>data</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>json</name>
                    <key>vfs.file.contents[/tmp/2.json]</key>
                    <delay>6s</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>data</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>vfs.file.contents[/tmp/data.txt]</name>
                    <key>vfs.file.contents[/tmp/data.txt]</key>
                    <delay>6s</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>data</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>lld.rule.plain</name>
                    <type>DEPENDENT</type>
                    <key>lld.rule.plain</key>
                    <delay>0</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#PATTERN}</macro>
                                <value>^\/.*\/$</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>0</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>key.plain[{#ID}]</name>
                            <type>DEPENDENT</type>
                            <key>key.plain[{#ID}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>if (value.search(&quot;{#PATTERN}&quot;) &gt; 0) {
return value;
}
else {
return 0
}
</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>vfs.file.contents[/tmp/data.txt]</key>
                            </master_item>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>vfs.file.contents[/tmp/2.json]</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#HOSTNAME}</lld_macro>
                            <path>$.host_name</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#ID}</lld_macro>
                            <path>$.id</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#PATTERN}</lld_macro>
                            <path>$.Pattern</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
                <discovery_rule>
                    <name>lld.rule.regex</name>
                    <type>DEPENDENT</type>
                    <key>lld.rule.regex</key>
                    <delay>0</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#PATTERN}</macro>
                                <value>^\/.*\/$</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>0</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>key.regex[{#ID}]</name>
                            <type>DEPENDENT</type>
                            <key>key.regex[{#ID}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>return value.match({#PATTERN}) ? value : 0;</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>vfs.file.contents[/tmp/data.txt]</key>
                            </master_item>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>vfs.file.contents[/tmp/2.json]</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#HOSTNAME}</lld_macro>
                            <path>$.host_name</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#ID}</lld_macro>
                            <path>$.id</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#PATTERN}</lld_macro>
                            <path>$.Pattern</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
