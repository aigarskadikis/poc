            <macros>
                <macro>
                    <macro>{$NETSTAT.STEP0}</macro>
                    <value>echo {&quot;data&quot;:[</value>
                </macro>
                <macro>
                    <macro>{$NETSTAT.STEP1}</macro>
                    <value>for /f &quot;tokens=2 delims= &quot; %f IN ('netstat -ano 0 ^| findstr &quot;TCP.*LISTENING&quot;')</value>
                </macro>
                <macro>
                    <macro>{$NETSTAT.STEP2}</macro>
                    <value>for /f &quot;tokens=1,2 delims=:&quot; %a in ('echo %f ^| findstr /B &quot;0.0.0.0:&quot;')</value>
                </macro>
                <macro>
                    <macro>{$NETSTAT.STEP3}</macro>
                    <value>for /f &quot;tokens=1 delims= &quot; %d IN ('echo %a&quot;,&quot;{#PORT}&quot;:&quot;%b')</value>
                </macro>
                <macro>
                    <macro>{$NETSTAT.STEP4}</macro>
                    <value>echo {&quot;{#IP}&quot;:&quot;%d&quot;},</value>
                </macro>
                <macro>
                    <macro>{$NETSTAT.STEP5}</macro>
